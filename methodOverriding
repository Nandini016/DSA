public class methodOverriding {
    public static void main(String args[]){
        KPOP k= new KPOP();
        BTS b = new BTS();
        k.sings();
        b.sings();

    }
    
}
class KPOP{
    void sings(){
        System.out.println("KPOP is Korean music");
    }
}
class BTS extends KPOP{
    void sings(){   // method overriding takes place
        System.out.println("BTS sings and make korean music");
    }
}
